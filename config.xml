<?xml version='1.1' encoding='utf-8'?>
<widget id="com.heppenstall.molarity" version="1.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.1">
    <name>molarity Calculator</name>
    <description>
        the best app ever.
    </description>
    <author email="heppenstall.co@gmail.com" href="http://heppenstall.atwebpages.com">
        Benjamin Heppenstall
    </author>
    <platform name="android">
    	<preference name="SplashMaintainAspectRatio" value="true"/>
    	<preference name="SplashShowOnlyFirstTime" value="true"/>
    	<icon density="ldpi" src="res/icon/android/ldpi.png"/>
    	<icon density="mdpi" src="res/icon/android/mdpi.png"/>
    	<icon density="hdpi" src="res/icon/android/hdpi.png"/>
    	<icon density="xhdpi" src="res/icon/android/xhdpi.png"/>
    	<icon density="xxhdpi" src="res/icon/android/xxhdpi.png"/>
    	<icon density="xxxhdpi" src="res/icon/android/xxxhdpi.png"/>
    	<splash density="port-ldpi" src="res/screen/android/splash-port-ldpi.png"/>
    	<splash density="port-mdpi" src="res/screen/android/splash-port-mdpi.png"/>
    	<splash density="port-hdpi" src="res/screen/android/splash-port-hdpi.png"/>
    	<splash density="port-xhdpi" src="res/screen/android/splash-port-xhdpi.png"/>
    	<splash density="port-xxhdpi" src="res/screen/android/splash-port-xxhdpi.png"/>
    	<splash density="port-xxxhdpi" src="res/screen/android/splash-port-xxxhdpi.png"/>
    </platform>
    <platform name="windows">
    	<preference name="SplashMaintainAspectRatio" value="true"/>
    	<preference name="SplashShowOnlyFirstTime" value="true"/>
    	<icon density="ldpi" src="res/icon//ldpi.png"/>
    	<icon density="mdpi" src="res/icon/windows/mdpi.png"/>
    	<icon density="hdpi" src="res/icon/windows/hdpi.png"/>
    	<icon density="xhdpi" src="res/icon/windows/xhdpi.png"/>
    	<icon density="xxhdpi" src="res/icon/windows/xxhdpi.png"/>
    	<icon density="xxxhdpi" src="res/icon/windows/xxxhdpi.png"/>
    	<splash density="port-ldpi" src="res/screen/windows/splash-port-ldpi.png"/>
    	<splash density="port-mdpi" src="res/screen/windows/splash-port-mdpi.png"/>
    	<splash density="port-hdpi" src="res/screen/windows/splash-port-hdpi.png"/>
    	<splash density="port-xhdpi" src="res/screen/windows/splash-port-xhdpi.png"/>
    	<splash density="port-xxhdpi" src="res/screen/windows/splash-port-xxhdpi.png"/>
    	<splash density="port-xxxhdpi" src="res/screen/windows/splash-port-xxxhdpi.png"/>
    </platform>
    <content src="index.html" />
    <access origin="*" />
</widget>
